{
  "name": "rollup-react-component-library",
  "version": "1.0.0",
  "description": "react ui components",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build:esm": "rollup -c rollup.esm.js --environment NODE_ENV:development",
    "copy-d.ts": "copyfiles -u 1 src/**/*.d.ts dist",
    "build:type": "tsc -b --force --verbose tsconfig.type.json && pnpm run copy-d.ts",
    "build:dev": "pnpm run build:type && rollup -c -w --environment NODE_ENV:development",
    "build:all": "rm -rf dist && pnpm run build:type && rollup -c --environment NODE_ENV:production"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.3",
    "@mui/material": "^5.10.3"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "7.13.0",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.0.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@sysuke/eslint-config-react": "workspace:*",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "copyfiles": "^2.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.79.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.8.1",
    "typescript": "^4.7.4"
  },
  "keywords": [
    "react",
    "react-component",
    "rollup",
    "@emotion"
  ],
  "author": "sysuke",
  "license": "ISC"
}
