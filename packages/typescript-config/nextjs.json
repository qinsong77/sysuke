{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next.js",
  "extends": "./base.json",
  "compilerOptions": {
    "plugins": [{ "name": "next" }],
    "module": "ESNext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "moduleResolution": "Bundler",
    "allowJs": false,
    "jsx": "preserve",
    "noEmit": true
  }
}
